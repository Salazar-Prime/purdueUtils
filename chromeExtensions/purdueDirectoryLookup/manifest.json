{
  "manifest_version": 3,
  "name": "Purdue Directory Lookup",
  "version": "0.1.0",
  "description": "Add a right clock context menu to loopup selection on Purdue Directory",
  "icons": {
    "48": "assets/icons8-search-client-48.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  }
}